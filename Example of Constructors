/*
The given program defines a Painting class and creates an object using the constructor.
Sample Input: Ocean
Sample Output: Ocean
*/

#include <iostream>
#include <string>
using namespace std;

class Painting {
    
    private:
        string name;
    
    public:
        Painting(string nm){
            name = nm;
            cout << nm;
        }
        string getName(){
            return name;
        }
};
int main() {
    string name;
    cin >> name;

    Painting painting(name);

    return 0;
}
